          "type": "boolean"
        },
        "sent_connection_request": {
          "id": "http://jsonschema.net/user/sent_connection_request",
          "type": "boolean"
        },
        "received_connection_request": {
          "type": "boolean"
        },
        "user_blocked_me": {
          "type": "boolean"

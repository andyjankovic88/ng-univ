            "type": "object",
            "properties": {
              "Name_of_School": {
                "type": "string"
              },
              "Dates_attended": {
                "type": "string"
              }
            }
          }
        },
        "work_experience": {
          "type": "array",
          "items": []
        },
        "places": {
          "type": "array",
          "items": []
        },
        "club_memberships": {
          "type": "array",
          "items": {
            "type": "array",
            "items": []
          }
        },
        "languages": {
          "type": "array",
          "items": {
            "type": "array",
            "items": []
          }
        },
        "classes": {
          "type": "array",

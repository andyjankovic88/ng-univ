            },
            "group_id": {
              "type": "integer"
            },
            "user_type": {
              "type": "string"
            }
          }
        },
        "photo_url": {
          "type": "string"
        },
        "group": {
          "type": "string"
        },
        "uni_id": {
          "type": "integer"
        },
        "gender": {
          "type": "string"
        },
        "commencement": {
          "type": "integer"
        },
        "year_of_completion": {
          "type": "integer"

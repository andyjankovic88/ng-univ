        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "event_image": {
          "type": "string"
        },
        "start_date": {
          "type": "string"
        },
        "start_date_hrs": {
          "type": "string"
        },
        "end_date": {
          "type": "string"
        },
        "end_date_hrs": {
          "type": "string"
        },
        "campus_id": {
          "type": "string"
        },
        "timezone": {
          "type": "string"
        },
        "location": {
